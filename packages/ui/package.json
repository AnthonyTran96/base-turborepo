{
  "name": "@repo/ui",
  "version": "0.0.0",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "files": [
    "dist",
    "src/**/*.scss",
    "src/assets"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*/index.js",
      "types": "./dist/*/index.d.ts"
    },
    "./styles": "./src/styles/index.scss",
    "./styles/*": "./src/styles/*",
    "./assets/*": "./src/assets/*"
  },
  "license": "MIT",
  "scripts": {
    "dev": "tsc --watch",
    "dev:mock": "tsc --watch",
    "build": "tsc",
    "build:dev": "tsc",
    "build:prod": "tsc",
    "check-types": "tsc --noEmit",
    "lint": "eslint src"
  },
  "peerDependencies": {
    "react": "^19",
    "next": "^15.4.2",
    "antd": "^5.23.1",
    "@repo/utils": "*",
    "moment": "^2.30.1",
    "react-hook-form": "^7.54.2"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/tailwind-config": "*",
    "@repo/typescript-config": "*",
    "@types/react": "^19.1.0",
    "eslint": "^9.33.0",
    "typescript": "5.9.2",
    "sass": "^1.83.4"
  }
}
