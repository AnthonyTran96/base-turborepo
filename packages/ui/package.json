{
  "name": "@repo/ui",
  "version": "0.0.0",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "files": [
    "dist",
    "src/**/*.scss"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*/index.js",
      "types": "./dist/*/index.d.ts"
    },
    "./styles": "./src/styles/index.scss",
    "./styles/*": "./src/styles/*"
  },
  "license": "MIT",
  "scripts": {
    "build:components": "tsc",
    "check-types": "tsc --noEmit",
    "dev:components": "tsc --watch",
    "lint": "eslint src --max-warnings 0"
  },
  "peerDependencies": {
    "react": "^19",
    "antd": "^5.23.1"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/tailwind-config": "*",
    "@repo/typescript-config": "*",
    "@types/react": "^19.1.0",
    "eslint": "^9.33.0",
    "typescript": "5.9.2",
    "sass": "^1.83.4"
  }
}
